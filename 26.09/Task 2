#include <algorithm>
#include <iostream>
#include <cmath>

using namespace std;

int main(){
      int a,b,c,d;
      cin>>a;
      cin>>b;
      cin>>c;
      cin>>d;
      if(a<1){
        cout<<"error";
        return 0;
      } else if(a>255){
        cout<<"error";  
        return 0;
      }
      if(b<1){
        cout<<"error";  
        return 0;
      } else if(b>255){
        cout<<"error"; 
        return 0;
      }
      if(c<1){
        cout<<"error"; 
        return 0;
      } else if(c>255){
        cout<<"error";  
        return 0;
      }
      if(d<1){
        cout<<"error";  
        return 0;
      } else if(d>255){
        cout<<"error";  
        return 0;
      }
      cout<<a<<"."<<b<<"."<<c<<"."<<d;
      return 0;
}
